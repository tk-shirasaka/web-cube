{
    "Uses" : {
        "Column" : {
            "Normal"    : "%s",
            "Type"      : "%s",
            "Length"    : "(%s)",
            "Null"      : "NOT NULL",
            "Default"   : "DEFAULT %s",
            "Constraint": "CONSTRAINT %s",
            "Attr"      : "%s"
        },
        "Constraints" : {
            "Primary"   : "PRIMARY KEY(%s)",
            "Foreign"   : "FOREIGN KEY(%s) REFERENCES %s (%s)",
            "Unique"    : "UNIQUE (%s)",
            "Check"     : "CHECK(%s)"
        },
        "Query" : {
            "Where"     : "WHERE %s",
            "Sort"      : "ORDER BY %s",
            "Group"     : "GROUP BY %s",
            "Limit"     : "LIMIT %s"
        }
    },
    "Show" : {
        "SQL" : {
            "Normal"    : "SHOW",
            "Database"  : "DATABASES",
            "Table"     : "TABLES",
            "Column"    : "COLUMNS FROM %s"
        }
    },
    "Create" : {
        "SQL" : {
            "Normal"     : "CREATE TABLE %s (%s)"
        },
        "Uses" : ["Column", "Constraints"]
    },
    "Drop" : {
        "SQL" : {
            "Normal"    : "DROP TABLE %s"
        }
    },
    "Alter" : {
        "SQL" : {
            "Normal"    : "ALTER TABLE %s",
            "Add"       : "ADD(%s)",
            "DropCol"   : "DROP(%s)",
            "DropAttr"  : "DROP %s",
            "Modify"    : "MODIFY(%s)",
            "Unuse"     : "SET UNUSED(%s)",
            "Enable"    : "ENABLE %s",
            "Disable"   : "DISABLE"
        },
        "Uses" : ["Column", "Constraints"]
    },
    "Constraint" : {
        "SQL" : {
            "Normal"    : "CONSTRAINT %s{%s}"
        },
        "Uses" : ["Constraints"]
    },
    "Insert" : {
        "SQL" : {
            "Normal"    : "INSERT INTO %s (%s) VALUES(%s)"
        }
    },
    "Update" : {
        "SQL" : {
            "Normal"    : "UPDATE %s SET %s"
        },
        "Uses" : ["Query"]
    },
    "Delete" : {
        "SQL" : {
            "Normal"    : "DELETE FROM %s"
        },
        "Uses" : ["Query"]
    },
    "Select" : {
        "SQL" : {
            "Normal"    : "SELECT %s FROM %s"
        },
        "Uses" : ["Query"]
    }
}
